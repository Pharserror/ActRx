{
  "name": "ActRx",
  "version": "0.2.1",
  "description": "An ActR plugin for RxJS style actions",
  "main": "dist/actrx.js",
  "author": "Adam Lind <sunboxnet@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Pharserror/ActRx",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pharserror/ActRx.git"
  },
  "keywords": [
    "Action",
    "Flux",
    "React",
    "Redux"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "test",
    "tests"
  ],
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "/test/",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "ActR": "git+ssh://git@github.com:Pharserror/ActR.git#master",
    "babel-jest": "22.4.3",
    "jest": "22.4.3",
    "jest-cli": "22.4.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "3.1.3"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "6.24.1",
    "webpack": "4.7.0"
  }
}
